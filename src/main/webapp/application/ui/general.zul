<zk xmlns:n="native">
  <n:style>
    .color-palette {
      height: 35px;
      line-height: 35px;
      text-align: center;
    }

    .color-palette-set {
      margin-bottom: 15px;
    }

    .color-palette span {
      display: none;
      font-size: 12px;
    }

    .color-palette:hover span {
      display: block;
    }

    .color-palette-box h4 {
      position: absolute;
      top: 100%;
      left: 25px;
      margin-top: -40px;
      color: rgba(255, 255, 255, 0.8);
      font-size: 12px;
      display: block;
      z-index: 7;
    }
  </n:style>
    <!-- Content Header (Page header) -->
    <n:section class="content-header">
      <n:h1>
        General UI
        <n:small>Preview of UI elements</n:small>
      </n:h1>
      <n:ol class="breadcrumb">
        <n:li><n:a href="#"><n:i class="fa fa-dashboard"></n:i> Home</n:a></n:li>
        <n:li><n:a href="#">UI</n:a></n:li>
        <n:li class="active">General</n:li>
      </n:ol>
    </n:section>

    <!-- Main content -->
    <n:section class="content">
