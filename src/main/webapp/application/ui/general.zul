      <!-- START ALERTS AND CALLOUTS -->
      <n:h2 class="page-header">Alerts and Callouts</n:h2>

      <n:div class="row">
        <n:div class="col-md-6">
          <n:div class="box box-default">
            <n:div class="box-header with-border">
              <n:i class="fa fa-warning"></n:i>

              <n:h3 class="box-title">Alerts</n:h3>
            </n:div>
            <!-- /.box-header -->
            <n:div class="box-body">
              <n:div class="alert alert-danger alert-dismissible">
                <n:button type="button" class="close" data-dismiss="alert" aria-hidden="true"><html><![CDATA[&times;]]></html></n:button>
                <n:h4><n:i class="icon fa fa-ban"></n:i> Alert!</n:h4>
                Danger alert preview. This alert is dismissable. A wonderful serenity has taken possession of my entire
                soul, like these sweet mornings of spring which I enjoy with my whole heart.
              </n:div>
              <n:div class="alert alert-info alert-dismissible">
                <n:button type="button" class="close" data-dismiss="alert" aria-hidden="true"><html><![CDATA[&times;]]></html></n:button>
                <n:h4><n:i class="icon fa fa-info"></n:i> Alert!</n:h4>
                Info alert preview. This alert is dismissable.
              </n:div>
              <n:div class="alert alert-warning alert-dismissible">
                <n:button type="button" class="close" data-dismiss="alert" aria-hidden="true"><html><![CDATA[&times;]]></html></n:button>
                <n:h4><n:i class="icon fa fa-warning"></n:i> Alert!</n:h4>
                Warning alert preview. This alert is dismissable.
              </n:div>
              <n:div class="alert alert-success alert-dismissible">
                <n:button type="button" class="close" data-dismiss="alert" aria-hidden="true"><html><![CDATA[&times;]]></html></n:button>
                <n:h4><n:i class="icon fa fa-check"></n:i> Alert!</n:h4>
                Success alert preview. This alert is dismissable.
              </n:div>
            </n:div>
            <!-- /.box-body -->
          </n:div>
          <!-- /.box -->
        </n:div>
        <!-- /.col -->

        <n:div class="col-md-6">
          <n:div class="box box-default">
            <n:div class="box-header with-border">
              <n:i class="fa fa-bullhorn"></n:i>

              <n:h3 class="box-title">Callouts</n:h3>
            </n:div>
            <!-- /.box-header -->
            <n:div class="box-body">
              <n:div class="callout callout-danger">
                <n:h4>I am a danger callout!</n:h4>

                <n:p>There is a problem that we need to fix. A wonderful serenity has taken possession of my entire soul,
                  like these sweet mornings of spring which I enjoy with my whole heart.</n:p>
              </n:div>
              <n:div class="callout callout-info">
                <n:h4>I am an info callout!</n:h4>

                <n:p>Follow the steps to continue to payment.</n:p>
              </n:div>
              <n:div class="callout callout-warning">
                <n:h4>I am a warning callout!</n:h4>

                <n:p>This is a yellow callout.</n:p>
              </n:div>
              <n:div class="callout callout-success">
                <n:h4>I am a success callout!</n:h4>

                <n:p>This is a green callout.</n:p>
              </n:div>
            </n:div>
            <!-- /.box-body -->
          </n:div>
          <!-- /.box -->
        </n:div>
        <!-- /.col -->
      </n:div>
      <!-- /.row -->
      <!-- END ALERTS AND CALLOUTS -->
