                  <n:th>Task</n:th>
                  <n:th>Progress</n:th>
                  <n:th style="width: 40px">Label</n:th>
                </n:tr>
                <n:tr>
                  <n:td>1.</n:td>
                  <n:td>Update software</n:td>
                  <n:td>
                    <n:div class="progress progress-xs">
                      <n:div class="progress-bar progress-bar-danger" style="width: 55%"></n:div>
                    </n:div>
                  </n:td>
